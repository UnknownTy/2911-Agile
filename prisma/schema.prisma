datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model Stats {
  id                     Int      @id @default(autoincrement())
  country                Country?  @relation(fields: [countryID], references: [id])
  countryID              Int?
  statsJSON              Json?
  updatedAt              DateTime?

}

model Country {
  id        Int      @id @default(autoincrement())
  name      String
  updatedAt DateTime?
  createdAt DateTime
  Stats     Stats[]
}
